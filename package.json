  "devDependencies": {
    "@types/angular": "^1.6.40",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.92",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.8",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.11.1",
    "jest": "^22.1.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.6.8",
    "lodash": "^4.17.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
